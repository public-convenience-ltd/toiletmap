    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run --headless",
    "cypress:headless:record": "cypress run --headless --record",
    "e2e": "./scripts/e2e.sh",
    "e2e:headless": "./scripts/e2e.sh -h",
    "e2e:dev": "./scripts/e2e.sh -d d",
    "wait-on:server": "yarn wait-on http://localhost:3000/ -t 60000",
    "wait-on:memoryMongo": "yarn wait-on http://localhost:9999/ -t 100000",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./public/storybook",
    "check": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write .",
    "migrate": "migrate-mongo up",
    "generate": "node -r dotenv/config ./src/api/db/manage/generate/index.js --confirm",
    "areas": "node -r dotenv/config ./src/api/db/manage/areaToDatabase/index.js",
    "startMemoryMongo": "MONGODB_URI=mongodb://localhost:27017/toiletmap TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' yarn ts-node --transpileOnly ./scripts/setupAndStartMemoryMongo.ts",
    "generateMocks": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' yarn ts-node --transpileOnly ./scripts/generateMocks.ts",
    "prepare": "husky install && cp ./node_modules/graphql-voyager/dist/voyager.worker.js ./public && yarn codegen",
    "codegen": "graphql-codegen --config codegen.yml"
  },
